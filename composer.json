{
  "name": "deuxhuithuit/craft-agency-auth",
  "description": "Adds SSO login in the cp with Google",
  "type": "craft-plugin",
  "version": "2.0.1",
  "keywords": [
    "admin",
    "control panel",
    "authentication",
    "sso"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Deux Huit Huit",
      "homepage": "https://deuxhuithuit.com"
    }
  ],
  "support": {
    "email": "open-source@deuxhuithuit.com",
    "issues": "https://github.com/DeuxHuitHuit/craft-agency-auth/issues?state=open",
    "source": "https://github.com/DeuxHuitHuit/craft-agency-auth",
    "docs": "https://github.com/DeuxHuitHuit/craft-agency-auth/blob/main/README.md"
  },
  "require": {
    "php": "8.1.*|8.2.*|8.3.*|8.4.*",
    "craftcms/cms": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "deuxhuithuit\\agencyauth\\": "src/"
    }
  },
  "extra": {
    "name": "Agency Auth",
    "handle": "agency-auth",
    "hasCpSettings": false
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "friendsofphp/php-cs-fixer": "^3.35",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
