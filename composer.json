{
  "name": "deuxhuithuit/craft-agency-auth",
  "description": "Adds SSO login in the cp with Google",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "admin",
    "control panel",
    "authentication",
    "sso"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Deux Huit Huit",
      "homepage": "https://deuxhuithuit.com"
    }
  ],
  "support": {
    "email": "open-source@deuxhuithuit.com",
    "issues": "https://github.com/DeuxHuitHuit/craft-agency-auth/issues?state=open",
    "source": "https://github.com/DeuxHuitHuit/craft-agency-auth",
    "docs": "https://github.com/DeuxHuitHuit/craft-agency-auth/blob/main/README.md"
  },
  "require": {
    "php": "8.1.*|8.2.*",
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "deuxhuithuit\\agencyauth\\": "src/"
    }
  },
  "extra": {
    "name": "Agency Auth",
    "handle": "agency-auth",
    "hasCpSettings": false
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.22"
  }
}
